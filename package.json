{
  "name": "mentalhealthv1",
  "version": "1.0.0",
  "main": "expo-router/entry",
"scripts": {
  "prestart": "node firebase-symlinks.js && node fix-exports.js && node prep-build.js",
  "start": "expo start --config ./rn-config-override.js --clear",
  "fix-exports": "node fix-exports.js",
  "fix-and-start": "node firebase-symlinks.js && npm run fix-exports && node prep-build.js && npx expo start --clear",
  "android": "node prep-build.js && expo start --android",
  "ios": "node prep-build.js && expo start --ios",
  "web": "node prep-build.js && expo start --web",
  "test": "jest --watchAll",
  "lint": "eslint .",
  "lint:fix": "eslint . --fix",
  "prebuild": "node prep-build.js && npm run fix-exports",
  "populate-db": "node populate-firestore.js"
},
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@firebase/auth": "^1.9.1",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-community/slider": "4.5.5",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "date-fns": "^4.1.0",
    "expo": "~52.0.38",
    "expo-av": "^15.0.2",
    "expo-constants": "~17.0.5",
    "expo-font": "~13.0.3",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.19",
    "expo-splash-screen": "~0.29.21",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.7",
    "firebase": "^11.5.0",
    "firebase-admin": "^13.2.0",
    "firebase-functions": "^6.3.2",
    "libphonenumber-js": "^1.11.20",
    "metro-core": "^0.81.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-paper": "^5.13.1",
    "react-native-paper-dates": "^0.22.34",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-web": "~0.19.6",
    "victory-native": "^41.16.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.3.12",
    "@types/react-native-vector-icons": "^6.4.18",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.56.0",
    "eslint-config-universe": "^12.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.6",
    "path-browserify": "^1.0.1",
    "prettier": "^3.2.4",
    "typescript": "^5.3.0"
  },
  "expo": {
    "name": "mentalhealthV1",
    "slug": "mentalhealthV1",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "firebase"
        ]
      }
    }
  },
  "private": true
}
